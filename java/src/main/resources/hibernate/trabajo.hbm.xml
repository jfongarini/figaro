<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.figaro.model.Trabajo" table="TRABAJO">
		<id name="id" type="int" column="ID_TRABAJO" >
			<generator class="increment"/>
		</id>
	 	<property name="descripcion" unique="true" column="DESCRIPCION" type="string"/>
	 	<property name="precio" column="PRECIO" type="big_decimal"/>
	</class>
</hibernate-mapping>
